{
  ".outline-view": {
    "enter": "pulsar-outline-view:activate-selected-entry",
    "left": "pulsar-outline-view:collapse-selected-entry",
    "escape": "pulsar-outline-view:unfocus",
    "home": "core:move-to-top",
    "end": "core:move-to-bottom"
  },

  ".platform-win32, .platform-linux": {
    "ctrl-alt-o": "pulsar-outline-view:show"
  },

  ".platform-darwin": {
    "ctrl-cmd-o": "pulsar-outline-view:show"
  }
}
